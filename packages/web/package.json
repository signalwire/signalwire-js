{
  "name": "@signalwire/web",
  "version": "0.0.0",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "exports": "./dist/index.modern.js",
  "module": "dist/index.esm.js",
  "unpkg": "dist/index.umd.js",
  "types": "dist/web/src/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
