{
  "name": "@signalwire/node",
  "private": true,
  "description": "SignalWire Node.js SDK",
  "author": "SignalWire Team <open.source@signalwire.com>",
  "license": "MIT",
  "version": "0.0.1-beta.2",
  "source": "src/index.ts",
  "exports": {
    "require": "./dist/index.node.js",
    "default": "./dist/index.node.mjs"
  },
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=12"
  },
  "keywords": [
    "signalwire",
    "video",
    "relay",
    "sip"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/signalwire/signalwire-js"
  },
  "bugs": {
    "url": "https://github.com/signalwire/signalwire-js/issues"
  },
  "homepage": "https://github.com/signalwire/signalwire-node/tree/master/packages/node",
  "scripts": {
    "start": "sw-build --dev --node",
    "build": "tsc --project tsconfig.build.json && sw-build --node",
    "test": "NODE_ENV=test jest",
    "prepare": "npm run build",
    "docs": "typedoc --options typedoc.js",
    "docs:watch": "typedoc --options typedoc.js --watch"
  },
  "dependencies": {
    "@signalwire/core": "3.0.0-beta.2",
    "node-abort-controller": "^2.0.0",
    "node-fetch": "^2.6.1",
    "ws": "^7.4.6"
  },
  "devDependencies": {
    "@types/node-fetch": "^2.5.10",
    "@types/ws": "^7.4.4",
    "msw": "^0.28.2",
    "ts-node": "^10.0.0"
  },
  "types": "dist/node/src/index.d.ts"
}
